.wkMain{  width:100%;height:1240rpx;background: white;display: flex; flex-wrap: wrap; justify-content: space-between;}
.wkMain .box{ width:3100%; height: 100%; margin-bottom:10rpx; position: relative;}
.wkMain .pic{ width: 100%; height: 100%;}
.wkMain .ceng{ width:100%;height: 100%; background: rgba(141, 139, 139, 0.5); position: absolute; top:0; left: 0; color:#fff; display: flex; flex-direction: column; justify-content: center; align-items: center; padding:0 20rpx; box-sizing: border-box;text-align: center;}

.wkMain .ceng .tit{font-size: 55rpx;}
.wkMain .ceng .line{ width:100rpx; height: 2rpx; background: #fff; margin:10rpx 0 20rpx;}
.wkMain .ceng .des{ opacity: 0.6; text-transform: uppercase; font-size:30rpx; letter-spacing:5rpx;}
